// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model AudioListings {
  id              Int    @id @default(autoincrement())
  name            String
  price           Int
  category        String
  channels        String
  description     String
  listerId        String
  audio           String
}

 